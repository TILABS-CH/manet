{
  "name": "tilabs/manet",
  "description": "A fluent class builder for Laravel blade components.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "role": "Developer",
      "name": "Thomas Iseli",
      "email": "hey@tilabs.ch",
      "homepage": "https://tilabs.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tilabs\\Manet\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tilabs\\Manet\\Providers\\ManetServiceProvider"
      ]
    }
  }
}
